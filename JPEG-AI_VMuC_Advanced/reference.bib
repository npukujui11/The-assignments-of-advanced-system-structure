@thesis{__2021-5,
	title = {新时代中国共产党自我革命的理论与实践研究},
	abstract = {中国共产党自成立起就领导和团结全国各族人民通过不懈奋斗,历经千辛万苦终于实现了从站起来、富起来到强起来的伟大飞跃。总结经验,中国共产党能够领导中国人民在革命、建设和改革时期取得重大历史性成就的原因,就在于不论处于顺境还是逆境,中国共产党人都始终勇于进行自我革命,敢于直面问题,坚持自我剖析,时刻为中国人民谋幸福,为中华民族谋复兴。当前,中国特色社会主义进入新时代,新时代使得中国梦的实现更加成为可能,与此同时,我们党正面临着重大风险和考验,世界形势复杂多变、改革发展任务重,对我们党提出了前所未有的考验。中国共产党作为百年大党,要始终得到人民的拥护和支持,书写中华民族的千秋伟业,必须勇于自我革命,以刀刃向内的勇气坚决清除一切弱化党的先进性、损害党的纯洁性的因素,使我们党成为领导人民进行革命、建设和改革的坚强领导核心。习近平总书记有着强烈的忧患意识,自党的十八大以来,他多次强调新时代共产党人要以自我革命的勇气防范党内存在的问题,并对新时代党的自我革命理论进行了系统全面的论述。党的自我革命既是一个理论问题,也是一个实践问题,只有立足于习近平关于党的自我革命重要论述的高度,坚持理论与实践的紧密结合,才能对新时代党的自我革命的依据、内涵、路径、价值等作出更加全面的阐释。本文正是基于这一思路,坚持理论与实践相结合,对新时代中国共产党自我革命这一命题展开研究。本文首先阐述了新时代党的自我革命理论提出的紧迫性和理论渊源,指出新时代党的自我革命并不是横空出世的,而是我们党在新时代应对复杂多变的国际形势,肩负党的历史使命,纯洁党的肌体的自觉选择。接下来,进一步从中国优秀传统文化、马克思主义经典作家们的自我革命思想以及中国共产党百年实践中形成的自我革命理论出发,深入挖掘新时代党的自我革命的理论根基。紧接着,阐述了新时代党的自我革命的丰富内涵和基本特征。即党的自我革命就是自我净化、自我完善、自我革新、自我提高。进而,通过分析其时代背景、理论基础和内涵体系,以问题性、指向性、继承性、创新性为切入点深刻剖析这一思想的基本特征。然后,结合新时代发展要求,从传统党建布局入手,本文主要从政治建设、思想建设、组织建设、作风建设、开展反腐败斗争等方面探讨了新时代深入推进党的自我革命的实践路径。最后,本文从理论价值和实践价值两方面深刻阐述了新时代中国共产党推进自我革命的重要价值。},
	institution = {吉林大学},
	type = {硕士, 10.27162/d.cnki.gjlin.2021.000998},
	author = {王红霞 and 朱哲},
	date = {2021},
	keywords = {中国共产党;党的建设;自我革命;初心使命},
	file = {新时代中国共产党自我革命的理论与实践研究_王红霞.caj:C\:\\Users\\90400\\Zotero\\storage\\2W6H6VPD\\新时代中国共产党自我革命的理论与实践研究_王红霞.caj:application/octet-stream},
}

@thesis{__2021-6,
	title = {新时代中国共产党自我革命研究},
	abstract = {中国特色社会主义如今进入了新时代,全面深化改革已经到了攻坚期。针对国内外复杂多变的形势,适时推进自我革命对于永葆党的纯洁性、先进性和生命力具有重要意义。对新时代党的自我革命进行研究,既是对马克思主义理论在新时代背景下的析解,也是对马克思主义中国化的继续深化,对推动社会主义事业的发展具有积极的指导意义。中国共产党的自我革命是革自己的命,是对其本身的革命,是“破”与“立”同行并举的过程。于“破”,自我革命是中国共产党的自我扬弃,即针对自身存在的问题,以破釜沉舟的勇气进行纠正。于“立”,自我革命是中国共产党完善体制机制的改革,实现理论、制度上的创新。新时代党自我革命的提出有其独特的历史背景和依据,有深远的理论基础、文化基础和政党学说基础。自我革命的马克思主义理论基础主要源自于马克思主义辨证否定观的内在要求、马克思主义实践观的源头活水以及马克思主义认识论的指导作用。作为在中国传统文化上成长起来的政党,中国共产党的自我革命也离不开传统文化的支撑,自我革命的推进离不开对国内外无产阶级政党和党建学说的总结。党的十八大开启了新时代中国共产党自我革命的历史,立足新的历史方位,党的自我革命面临着能力不足、精神懈怠、脱离群众的挑战。因此,新时代推进自我革命是筑牢信仰之基的需要,是推进党的的建设的需要,是进行伟大斗争的需要,是坚定理论自信的需要,也是净化政治生态的需要。新时代中国共产党的自我革命以全面深化改革、全面从严治党和专题学习教育为主要内容,彰显出符合时代特色的刀刃向内改造主观世界、基因传承弘扬优良传统、人民至上不忘初心使命的特征。实现新时代党的自我革命时,要坚持正确的革命方向,继续完善党的制度建设,发挥党内典型作用,从而保障党的新时代自我革命顺利推进。新时代党进行自我革命具有深刻的现实和历史意义:它为解决自身存在的问题找到了有效途径,为社会革命提供了强大动力,也为世界政党治理提供了中国方案。},
	institution = {延安大学},
	type = {硕士, 10.27438/d.cnki.gyadu.2021.000187},
	author = {朱晓菲 and 李宏斌},
	date = {2021},
	keywords = {自我革命;理论依据;实现路径},
	file = {新时代中国共产党自我革命研究_朱晓菲.caj:C\:\\Users\\90400\\Zotero\\storage\\7E2DY9IM\\新时代中国共产党自我革命研究_朱晓菲.caj:application/octet-stream},
}

@thesis{__2022-6,
	title = {新时代中国共产党自我革命研究},
	abstract = {总结和反思中国共产党百年历史,我们看到党的发展史上的高潮和低潮以及当前处于百年未有之大变局的现状;我们意识到蜕化变质是造成党面临执政风险和治理危机的主要内因;我们认识到历史地看待党的发展既要反对盲目地肯定一切也要反对盲目地否定一切。因此,中国共产党自我革命具有重大意义。那么,究竟应该如何理解和推进党的自我革命?这是一个重要的理论问题、历史问题和实践问题。我们要运用“坚持文献研究与现实关注相统一、坚持历史方法与辩证方法相统一、坚持理论继承与实践创新相统一”的方法,在面向实践、面向人民、面向历史、面向现实、面向未来中理解和把握党的自我革命,在自觉、自信、自为中深化和推进党的自我革命。新时代中国共产党自我革命作为一个重大的理论命题,我们要明晰中国共产党自我革命的概念范畴,追溯中国共产党自我革命的理论渊源,把握中国共产党自我革命的发展历史。第一,关于中国共产党自我革命概念范畴的研究。本文围绕关于党的自我革命所展开的争论,对与之相似、相关、相近的概念关系进行辨析,通过讲清楚“自我革命是什么”或“自我革命不是什么”来确定其概念的边界与外延。在此基础上进一步对自我革命进行单刀直入、细致入微的解剖,直接回答自我革命的内涵、内容、功能、原则、动力、特征是什么。第二,关于中国共产党自我革命理论渊源的研究。本文认为,马克思主义的革命理论、唯物辩证法和无产阶级政党建设理论是党自我革命的理论基础,中华优秀传统文化是党自我革命的文化渊源,因此,党的自我革命是马克思主义基本原理同中华优秀传统文化相结合的产物。第三,关于中国共产党自我革命历史考察的研究。本文认为,自我革命作为党的历史的重要组成部分,在不同的历史时期具有不同的实践形式和内容,这既取决于各个历史时期的具体历史环境和时代任务,也取决于各个历史时期党的革命战略和策略,还取决于各个历史时期党的领导人的见识、格局和能力。本文通过系统梳理和总结党的自我革命历史,从中得出关于推进党的自我革命的历史启示和智慧。新时代中国共产党自我革命作为一个重大的实践命题,我们要对新时代中国共产党推进自我革命进行实践考察、态势分析和路径探析。第一,关于新时代中国共产党自我革命的实践考察。本文系统地梳理了新时代党进行自我革命的主要实践活动,总结了党通过自我革命在党的建设、党的领导和党的执政三个方面所取得的巨大实践成效,进而阐述了新时代党推进自我革命的基本经验。第二,关于新时代中国共产党推进自我革命的态势分析。本文认为,在新时代要把党的自我革命引向深入就必须深刻把握当前党的自我革命态势,即要把握党推进自我革命的优势、劣势、机会和威胁等元素。本文指出,变化的世界和变革的中国是当前党推进自我革命面临的最大的“实际”,我们要着力破解制约党推进自我革命的现实问题,立足于党推进自我革命的坚实基础,相信党的自我革命在新时代会更加成熟。第三,关于新时代中国共产党推进自我革命的路径探析。本文认为,新时代党推进自我革命能够凸显马克思主义政党的显著优势、把握治党理政的历史主动和推动完成“三大时代课题”,必须坚持理论发展与实践深化相结合、坚持组织强化与个人自律相结合、坚持顶层设计与基层探索相结合、坚持问题导向与目标导向相结合、坚持全面系统与重点突出相结合、坚持遵循规律与大胆创新相结合,在具体实践中科学增强自我革命意识、系统提升自我革命能力、全面激活自我革命动力、创新优化自我革命方法、有效执行自我革命制度、构建提升自我革命话语,以此“将自我革命进行到底”,不断把新时代党的自我革命推向深入。中国共产党兼具革命党、领导党和执政党三重属性,履行多重治理功能,肩负民族复兴历史使命,党的伟大光荣正确之处就在于具有一种在关键时刻超越自身局限性的品质,把自我擢升到历史向我们提出的紧迫任务的高度。我们要对党的建设和党的领导充满坚定和信心,对党不忠诚、不信任、不认同容易被利用也容易被奴役,如果这个世界有自由、民主、解放,那么一定是中国共产党才能带领人民实现的。社会从未像当代中国这样,如此彰显社会主义的优越性、共产主义的生命力、共产党的领导力。我们要坚持自我革命,把我们党建设成为世界上最强大的政党!},
	institution = {兰州大学},
	type = {博士, 10.27204/d.cnki.glzhu.2022.002756},
	author = {陈慧瑞 and 刘先春},
	date = {2022},
	keywords = {中国共产党;自我革命;新时代;态势;路径},
	file = {新时代中国共产党自我革命研究_陈慧瑞.caj:C\:\\Users\\90400\\Zotero\\storage\\KX2FFSXG\\新时代中国共产党自我革命研究_陈慧瑞.caj:application/octet-stream},
}

