@article{__2020-2,
	title = {新时代中国共产党跳出历史周期率的成功之路——学习习近平总书记关于全面从严治党的重要论述},
	volume = {36},
	abstract = {历史周期率是黄炎培基于家、国、团体兴衰现象向毛泽东提出的历史问题。就中国共产党来说,历史周期率与两个阶级、两条道路、两种社会制度的斗争联系在一起。中国共产党长期执政的地位和使命与跳出历史周期率合二为一。以毛泽东为核心的中国共产党对跳出历史周期率开辟出航道。跳出历史周期率、实现长期执政,是新时代中国共产党接续探索的课题。从根本上说,跳出历史周期率问题伴随统筹推进"四个伟大"的历史进程。全面从严治党是新时代党跳出历史周期率的战略举措和成功实践。以自我革命精神推进党的建设伟大工程,永葆党的先进性和纯洁性,把党锻炼成坚强的马克思主义政党,是破解历史周期率的治本之策。},
	pages = {26--35},
	number = {2},
	journaltitle = {治理现代化研究},
	author = {杨璐, 刘仓 {\textbackslash}\%A},
	date = {2020},
	note = {{ISBN}: 2096-5729},
	keywords = {新时代;中国共产党;历史周期率;全面从严治党;自我革命},
	file = {杨璐 - 2020 - 新时代中国共产党跳出历史周期率的成功之路——学习习近平总书记关于全面从严治党的重要论述.pdf:C\:\\Users\\90400\\Zotero\\storage\\ZQZURTWN\\杨璐 - 2020 - 新时代中国共产党跳出历史周期率的成功之路——学习习近平总书记关于全面从严治党的重要论述.pdf:application/pdf},
}

@thesis{__2022-7,
	title = {习近平关于跳出“历史周期率”的重要论述研究},
	abstract = {“历史周期率”课题产生于1945年民主人士黄炎培和毛泽东在延安进行的“窑洞对答”。此后,对跳出“历史周期率”的探索就与中国共产党的政党实践同气连枝,互融并进。对“历史周期率”课题的破解,既体现了中国共产党人对保持党的旺盛生机的主观愿望和执着追求,也见证了中国共产党人对共产党长期执政规律、社会主义建设规律和人类社会发展规律认识的不断深化。作为新一代党的最高领导人,习近平对“历史周期率”课题高度关注,作了立足于时代方位和现实条件的全面审视。习近平在思考“历史周期率”课题时,问题意识更加鲜明、忧患意识更加强烈、时代特征更加显著。尤其在党的十八大之后,习近平在各个场合多次谈及“历史周期率”课题,由此形成的重要论述聚焦实现党的长期执政和国家的长治久安,对社会主义国家发展及其政党建设作了新的时代谋划,具有极高的现实意义和思想引领价值。论文尝试从以下视角对习近平关于跳出“历史周期率”的重要论述展开研究。对“历史周期率”的基础理论有一个较为明晰的认知,是对习近平关于跳出“历史周期率”的重要论述进行研究的学理前提。论文在学理基础研究部分,首先分析了“周期率”的词源学含义,在无机界运动、生物运动、人类社会运动视角下对“周期率”现象作了多维审视。接着,考察“历史周期率”的话语出场,对“历史周期率”对话文本进行解读。最后,辨析“历史周期率”与“历史周期律”,讨论“历史周期率”与“历史周期律”各自的使用边界。通过对“历史周期率”的基础理论分析,奠定习近平关于跳出“历史周期率”的重要论述的学理基础。习近平关于跳出“历史周期率”的重要论述具有深厚的理论条件、历史基础和现实依据,三者共同构成了习近平关于跳出“历史周期率”重要论述的生成逻辑。其一,对马克思主义无产阶级政党学说的继承与创新、对中华优秀传统文化中积淀的为政兴国之道的赓续与发展、对当代世界政党活动文明成果的汲取与借鉴为该重要论述提供了思想资源。其二,中国共产党人对跳出“历史周期率”的探索为该重要论述奠定了历史基础。其三,应对世界百年未有之大变局、助力中华民族走向伟大复兴、推进党的建设新的伟大工程是新时代中国共产党跳出“历史周期率”的现实依据。在理论、文化、历史、现实的交融与互动中,习近平以高瞻远瞩的宏观视野深刻把握无产阶级政党立党命脉,汲取运用中华优秀文化精髓,总结中国共产党不负初心、践行使命的奋斗经验,聚焦思考党和国家发展的急难险要问题,为新时代中国共产党人破解“历史周期率”课题开阔了视野疆域,引领了行动方向。把握习近平关于跳出“历史周期率”的重要论述的发展过程,能够将这一论述的演进历程可视化呈现,也为领会该论述宏旨创造了一个切入口。习近平关于“历史周期率”的思考是一个迭代优化、继承发展的递进过程。这一重要论述经历了党的十八大以前的思想酝酿期、党的十八大到党的十九大的初步形成期、党的十九大以后的进一步发展三个阶段。习近平在继承中国共产党人探索跳出“历史周期率”的经验的基础上,创造性地融入丰富的实践经历和深入思考,对中国共产党跳出“历史周期率”的认识逐步趋向深化、丰富与全面。当前,习近平关于跳出“历史周期率”的重要论述仍在发展完善之中,体系内各要素也要结合世情、国情、党情的发展变化作联动性更强、耦合度更高的配套升级。这一重要论述的理论价值和现实意义更加突显,在未来仍有极大的完善发展空间。总结习近平关于中国共产党跳出“历史周期率”的重要论述原创性贡献是论文研究的重点指向。其原创性贡献体现三个方面。一是设定中国共产党跳出“历史周期率”的目标愿景:保证中国特色社会主义巍巍巨轮行稳致远是宏观目标;保持中国共产党不变质、不变色、不变味是中观理想;提高中国共产党领导水平和执政能力是微观愿景。二是树立中国共产党跳出“历史周期率”的基本原则:坚守“以人民为中心”的价值尺度、遵循“问题是时代呼声”的思路导向、树立“赶考永远在路上”的精神自觉、设定“打铁还需自身硬”的素质要求是中国共产党人破解这一历史性课题的四个原则遵循。三是擘画中国共产党跳出“历史周期率”的实践图景:以党的自我革命引领社会革命为总体思路;以高质量发展全过程人民民主为关键环节;以推动改革发展成果惠及人民为绩效支撑;以强化对权力运行的监督制约为重要举措;以培养高素质人才队伍为组织保障。由此,习近平廓清了新时代中国共产党跳出“历史周期率”的重点任务,标定了跳出“历史周期率”的基本原则,制定了破解“历史周期率”课题的因应之道。这些原创性贡献是习近平关于跳出“历史周期率”的重要论述的精髓所在。习近平关于跳出“历史周期率”的重要论述中内含多种灵活、科学的思维方法。表现为高屋建瓴的战略思维与把握矛盾的辩证思维相贯通,以史为鉴的历史思维与知危图安的底线思维相协同,革故鼎新的创新思维与建规立制的法治思维相联动的显著特征。由此彰显了新时代中国共产党人运用唯物辩证法分析问题、解决问题的能力和水平,树立了21世纪马克思主义者审察全局、研判矛盾、追求真理、捍卫正义、因势而动的光辉典范。习近平关于跳出“历史周期率”的重要论述具有重大的时代价值,主要包括理论与实践两个方面。在理论价值方面,习近平关于跳出“历史周期率”的重要论述深化了中国共产党对共产党执政、社会主义建设、人类社会发展规律的认识;丰富了习近平新时代中国特色社会主义思想的科学内涵;推动了马克思主义党建学说创新发展。在实践价值方面,习近平关于跳出“历史周期率”的重要论述为党和国家稳定安全大局铸就了坚实基座,为社会主义现代化建设提供了行动指引,为世界其他政党破解自我监督难题贡献了中国智慧。由此,习近平关于跳出“历史周期率”的重要论述的理论感召力得以激活,现实引领力显著提升。},
	institution = {西南大学},
	type = {硕士, 10.27684/d.cnki.gxndx.2022.000226},
